@html.extend('./layout', function(model){
  @html.block("body", function(model){
  @if(model.error)
  {
    <p>Error occured: @model.error</p>
  }
      
  <div id="wrapper" >
    <div id="slider" class="flexslider" style="margin: 0 auto;">      
         @html.include('./modules/slider', model)
    </div>
  </div>
  
  })

  @html.block("scripts", function(model){        
    <script type="text/javascript" src="lib/require.js" data-main="lib/home.js"></script>
  })
})